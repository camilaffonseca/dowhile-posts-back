{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./src/",
    "noImplicitAny": true,
    "typeRoots": ["./src/@types", "node_modules/@types"]
  },
  "exclude": ["node_modules"],
}
